<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * TestateRassegnaStampaRss
 */
class TestateRassegnaStampaRss
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $testata;

    /**
     * @var string
     */
    private $codice;

    /**
     * @var string
     */
    private $logoBig;

    /**
     * @var string
     */
    private $logoSmall;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set testata
     *
     * @param string $testata
     * @return TestateRassegnaStampaRss
     */
    public function setTestata($testata)
    {
        $this->testata = $testata;

        return $this;
    }

    /**
     * Get testata
     *
     * @return string 
     */
    public function getTestata()
    {
        return $this->testata;
    }

    /**
     * Set codice
     *
     * @param string $codice
     * @return TestateRassegnaStampaRss
     */
    public function setCodice($codice)
    {
        $this->codice = $codice;

        return $this;
    }

    /**
     * Get codice
     *
     * @return string 
     */
    public function getCodice()
    {
        return $this->codice;
    }

    /**
     * Set logoBig
     *
     * @param string $logoBig
     * @return TestateRassegnaStampaRss
     */
    public function setLogoBig($logoBig)
    {
        $this->logoBig = $logoBig;

        return $this;
    }

    /**
     * Get logoBig
     *
     * @return string 
     */
    public function getLogoBig()
    {
        return $this->logoBig;
    }

    /**
     * Set logoSmall
     *
     * @param string $logoSmall
     * @return TestateRassegnaStampaRss
     */
    public function setLogoSmall($logoSmall)
    {
        $this->logoSmall = $logoSmall;

        return $this;
    }

    /**
     * Get logoSmall
     *
     * @return string 
     */
    public function getLogoSmall()
    {
        return $this->logoSmall;
    }
}
