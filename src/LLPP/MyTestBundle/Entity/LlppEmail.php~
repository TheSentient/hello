<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * LlppEmail
 */
class LlppEmail
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $sezione;

    /**
     * @var string
     */
    private $spiegazione;

    /**
     * @var string
     */
    private $da;

    /**
     * @var string
     */
    private $nomeMittente;

    /**
     * @var string
     */
    private $html;

    /**
     * @var string
     */
    private $oggetto;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set sezione
     *
     * @param string $sezione
     * @return LlppEmail
     */
    public function setSezione($sezione)
    {
        $this->sezione = $sezione;

        return $this;
    }

    /**
     * Get sezione
     *
     * @return string 
     */
    public function getSezione()
    {
        return $this->sezione;
    }

    /**
     * Set spiegazione
     *
     * @param string $spiegazione
     * @return LlppEmail
     */
    public function setSpiegazione($spiegazione)
    {
        $this->spiegazione = $spiegazione;

        return $this;
    }

    /**
     * Get spiegazione
     *
     * @return string 
     */
    public function getSpiegazione()
    {
        return $this->spiegazione;
    }

    /**
     * Set da
     *
     * @param string $da
     * @return LlppEmail
     */
    public function setDa($da)
    {
        $this->da = $da;

        return $this;
    }

    /**
     * Get da
     *
     * @return string 
     */
    public function getDa()
    {
        return $this->da;
    }

    /**
     * Set nomeMittente
     *
     * @param string $nomeMittente
     * @return LlppEmail
     */
    public function setNomeMittente($nomeMittente)
    {
        $this->nomeMittente = $nomeMittente;

        return $this;
    }

    /**
     * Get nomeMittente
     *
     * @return string 
     */
    public function getNomeMittente()
    {
        return $this->nomeMittente;
    }

    /**
     * Set html
     *
     * @param string $html
     * @return LlppEmail
     */
    public function setHtml($html)
    {
        $this->html = $html;

        return $this;
    }

    /**
     * Get html
     *
     * @return string 
     */
    public function getHtml()
    {
        return $this->html;
    }

    /**
     * Set oggetto
     *
     * @param string $oggetto
     * @return LlppEmail
     */
    public function setOggetto($oggetto)
    {
        $this->oggetto = $oggetto;

        return $this;
    }

    /**
     * Get oggetto
     *
     * @return string 
     */
    public function getOggetto()
    {
        return $this->oggetto;
    }
}
