<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * EbookOfferta
 */
class EbookOfferta
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $isbn;

    /**
     * @var float
     */
    private $prezzo;

    /**
     * @var string
     */
    private $titolo;

    /**
     * @var string
     */
    private $descrizione;

    /**
     * @var integer
     */
    private $numeroPagine;

    /**
     * @var string
     */
    private $dimensioneEpub;

    /**
     * @var integer
     */
    private $impression;

    /**
     * @var integer
     */
    private $click;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set isbn
     *
     * @param string $isbn
     * @return EbookOfferta
     */
    public function setIsbn($isbn)
    {
        $this->isbn = $isbn;

        return $this;
    }

    /**
     * Get isbn
     *
     * @return string 
     */
    public function getIsbn()
    {
        return $this->isbn;
    }

    /**
     * Set prezzo
     *
     * @param float $prezzo
     * @return EbookOfferta
     */
    public function setPrezzo($prezzo)
    {
        $this->prezzo = $prezzo;

        return $this;
    }

    /**
     * Get prezzo
     *
     * @return float 
     */
    public function getPrezzo()
    {
        return $this->prezzo;
    }

    /**
     * Set titolo
     *
     * @param string $titolo
     * @return EbookOfferta
     */
    public function setTitolo($titolo)
    {
        $this->titolo = $titolo;

        return $this;
    }

    /**
     * Get titolo
     *
     * @return string 
     */
    public function getTitolo()
    {
        return $this->titolo;
    }

    /**
     * Set descrizione
     *
     * @param string $descrizione
     * @return EbookOfferta
     */
    public function setDescrizione($descrizione)
    {
        $this->descrizione = $descrizione;

        return $this;
    }

    /**
     * Get descrizione
     *
     * @return string 
     */
    public function getDescrizione()
    {
        return $this->descrizione;
    }

    /**
     * Set numeroPagine
     *
     * @param integer $numeroPagine
     * @return EbookOfferta
     */
    public function setNumeroPagine($numeroPagine)
    {
        $this->numeroPagine = $numeroPagine;

        return $this;
    }

    /**
     * Get numeroPagine
     *
     * @return integer 
     */
    public function getNumeroPagine()
    {
        return $this->numeroPagine;
    }

    /**
     * Set dimensioneEpub
     *
     * @param string $dimensioneEpub
     * @return EbookOfferta
     */
    public function setDimensioneEpub($dimensioneEpub)
    {
        $this->dimensioneEpub = $dimensioneEpub;

        return $this;
    }

    /**
     * Get dimensioneEpub
     *
     * @return string 
     */
    public function getDimensioneEpub()
    {
        return $this->dimensioneEpub;
    }

    /**
     * Set impression
     *
     * @param integer $impression
     * @return EbookOfferta
     */
    public function setImpression($impression)
    {
        $this->impression = $impression;

        return $this;
    }

    /**
     * Get impression
     *
     * @return integer 
     */
    public function getImpression()
    {
        return $this->impression;
    }

    /**
     * Set click
     *
     * @param integer $click
     * @return EbookOfferta
     */
    public function setClick($click)
    {
        $this->click = $click;

        return $this;
    }

    /**
     * Get click
     *
     * @return integer 
     */
    public function getClick()
    {
        return $this->click;
    }
}
