<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Lninfocampi
 */
class Lninfocampi
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $ordine;

    /**
     * @var string
     */
    private $testo;

    /**
     * @var string
     */
    private $nomecampo;

    /**
     * @var string
     */
    private $tipo;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set ordine
     *
     * @param integer $ordine
     * @return Lninfocampi
     */
    public function setOrdine($ordine)
    {
        $this->ordine = $ordine;

        return $this;
    }

    /**
     * Get ordine
     *
     * @return integer 
     */
    public function getOrdine()
    {
        return $this->ordine;
    }

    /**
     * Set testo
     *
     * @param string $testo
     * @return Lninfocampi
     */
    public function setTesto($testo)
    {
        $this->testo = $testo;

        return $this;
    }

    /**
     * Get testo
     *
     * @return string 
     */
    public function getTesto()
    {
        return $this->testo;
    }

    /**
     * Set nomecampo
     *
     * @param string $nomecampo
     * @return Lninfocampi
     */
    public function setNomecampo($nomecampo)
    {
        $this->nomecampo = $nomecampo;

        return $this;
    }

    /**
     * Get nomecampo
     *
     * @return string 
     */
    public function getNomecampo()
    {
        return $this->nomecampo;
    }

    /**
     * Set tipo
     *
     * @param string $tipo
     * @return Lninfocampi
     */
    public function setTipo($tipo)
    {
        $this->tipo = $tipo;

        return $this;
    }

    /**
     * Get tipo
     *
     * @return string 
     */
    public function getTipo()
    {
        return $this->tipo;
    }
}
