<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Lnblocchicad
 */
class Lnblocchicad
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $idCliente;

    /**
     * @var string
     */
    private $categoria;

    /**
     * @var string
     */
    private $titolo;

    /**
     * @var string
     */
    private $descrizione;

    /**
     * @var string
     */
    private $imganteprima;

    /**
     * @var string
     */
    private $bloccocad;

    /**
     * @var \DateTime
     */
    private $data;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set idCliente
     *
     * @param integer $idCliente
     * @return Lnblocchicad
     */
    public function setIdCliente($idCliente)
    {
        $this->idCliente = $idCliente;

        return $this;
    }

    /**
     * Get idCliente
     *
     * @return integer 
     */
    public function getIdCliente()
    {
        return $this->idCliente;
    }

    /**
     * Set categoria
     *
     * @param string $categoria
     * @return Lnblocchicad
     */
    public function setCategoria($categoria)
    {
        $this->categoria = $categoria;

        return $this;
    }

    /**
     * Get categoria
     *
     * @return string 
     */
    public function getCategoria()
    {
        return $this->categoria;
    }

    /**
     * Set titolo
     *
     * @param string $titolo
     * @return Lnblocchicad
     */
    public function setTitolo($titolo)
    {
        $this->titolo = $titolo;

        return $this;
    }

    /**
     * Get titolo
     *
     * @return string 
     */
    public function getTitolo()
    {
        return $this->titolo;
    }

    /**
     * Set descrizione
     *
     * @param string $descrizione
     * @return Lnblocchicad
     */
    public function setDescrizione($descrizione)
    {
        $this->descrizione = $descrizione;

        return $this;
    }

    /**
     * Get descrizione
     *
     * @return string 
     */
    public function getDescrizione()
    {
        return $this->descrizione;
    }

    /**
     * Set imganteprima
     *
     * @param string $imganteprima
     * @return Lnblocchicad
     */
    public function setImganteprima($imganteprima)
    {
        $this->imganteprima = $imganteprima;

        return $this;
    }

    /**
     * Get imganteprima
     *
     * @return string 
     */
    public function getImganteprima()
    {
        return $this->imganteprima;
    }

    /**
     * Set bloccocad
     *
     * @param string $bloccocad
     * @return Lnblocchicad
     */
    public function setBloccocad($bloccocad)
    {
        $this->bloccocad = $bloccocad;

        return $this;
    }

    /**
     * Get bloccocad
     *
     * @return string 
     */
    public function getBloccocad()
    {
        return $this->bloccocad;
    }

    /**
     * Set data
     *
     * @param \DateTime $data
     * @return Lnblocchicad
     */
    public function setData($data)
    {
        $this->data = $data;

        return $this;
    }

    /**
     * Get data
     *
     * @return \DateTime 
     */
    public function getData()
    {
        return $this->data;
    }
