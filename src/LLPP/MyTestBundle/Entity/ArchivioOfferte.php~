        return $this->stoffSpedizione;
    }

    /**
     * Set stoffPrezzoOfferta
     *
     * @param float $stoffPrezzoOfferta
     * @return ArchivioOfferte
     */
    public function setStoffPrezzoOfferta($stoffPrezzoOfferta)
    {
        $this->stoffPrezzoOfferta = $stoffPrezzoOfferta;

        return $this;
    }

    /**
     * Get stoffPrezzoOfferta
     *
     * @return float 
