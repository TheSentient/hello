<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ArchivioNewsletterAziende
 */
class ArchivioNewsletterAziende
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $codice;

    /**
     * @var string
     */
    private $azienda;

    /**
     * @var \DateTime
     */
    private $dataSpedizione;

    /**
     * @var string
     */
    private $linkEseguire;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set codice
     *
     * @param string $codice
     * @return ArchivioNewsletterAziende
     */
    public function setCodice($codice)
    {
        $this->codice = $codice;

        return $this;
    }

    /**
     * Get codice
     *
     * @return string 
     */
    public function getCodice()
    {
        return $this->codice;
    }

    /**
     * Set azienda
     *
     * @param string $azienda
     * @return ArchivioNewsletterAziende
     */
    public function setAzienda($azienda)
    {
        $this->azienda = $azienda;

        return $this;
    }

    /**
     * Get azienda
     *
     * @return string 
     */
    public function getAzienda()
    {
        return $this->azienda;
    }

    /**
     * Set dataSpedizione
     *
     * @param \DateTime $dataSpedizione
     * @return ArchivioNewsletterAziende
     */
    public function setDataSpedizione($dataSpedizione)
    {
        $this->dataSpedizione = $dataSpedizione;

        return $this;
    }

    /**
     * Get dataSpedizione
     *
     * @return \DateTime 
     */
    public function getDataSpedizione()
    {
        return $this->dataSpedizione;
    }

    /**
     * Set linkEseguire
     *
     * @param string $linkEseguire
     * @return ArchivioNewsletterAziende
     */
    public function setLinkEseguire($linkEseguire)
    {
        $this->linkEseguire = $linkEseguire;

        return $this;
    }

    /**
     * Get linkEseguire
     *
     * @return string 
     */
    public function getLinkEseguire()
    {
        return $this->linkEseguire;
    }
}
