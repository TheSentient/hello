<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Lngruppiinterventi
 */
class Lngruppiinterventi
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $idGruppo;

    /**
     * @var string
     */
    private $titolo;

    /**
     * @var string
     */
    private $testo;

    /**
     * @var \DateTime
     */
    private $data;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set idGruppo
     *
     * @param integer $idGruppo
     * @return Lngruppiinterventi
     */
    public function setIdGruppo($idGruppo)
    {
        $this->idGruppo = $idGruppo;

        return $this;
    }

    /**
     * Get idGruppo
     *
     * @return integer 
     */
    public function getIdGruppo()
    {
        return $this->idGruppo;
    }

    /**
     * Set titolo
     *
     * @param string $titolo
     * @return Lngruppiinterventi
     */
    public function setTitolo($titolo)
    {
        $this->titolo = $titolo;

        return $this;
    }

    /**
     * Get titolo
     *
     * @return string 
     */
    public function getTitolo()
    {
        return $this->titolo;
    }

    /**
     * Set testo
     *
     * @param string $testo
     * @return Lngruppiinterventi
     */
    public function setTesto($testo)
    {
        $this->testo = $testo;

        return $this;
    }

    /**
     * Get testo
     *
     * @return string 
     */
    public function getTesto()
    {
        return $this->testo;
    }

    /**
     * Set data
     *
     * @param \DateTime $data
     * @return Lngruppiinterventi
     */
    public function setData($data)
    {
        $this->data = $data;

        return $this;
    }

    /**
     * Get data
     *
     * @return \DateTime 
     */
    public function getData()
    {
        return $this->data;
    }
}
