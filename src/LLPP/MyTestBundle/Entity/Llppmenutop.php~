<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Llppmenutop
 */
class Llppmenutop
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $testomenu;

    /**
     * @var string
     */
    private $tipo;

    /**
     * @var string
     */
    private $testopadre;

    /**
     * @var integer
     */
    private $ordine;

    /**
     * @var string
     */
    private $link;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set testomenu
     *
     * @param string $testomenu
     * @return Llppmenutop
     */
    public function setTestomenu($testomenu)
    {
        $this->testomenu = $testomenu;

        return $this;
    }

    /**
     * Get testomenu
     *
     * @return string 
     */
    public function getTestomenu()
    {
        return $this->testomenu;
    }

    /**
     * Set tipo
     *
     * @param string $tipo
     * @return Llppmenutop
     */
    public function setTipo($tipo)
    {
        $this->tipo = $tipo;

        return $this;
    }

    /**
     * Get tipo
     *
     * @return string 
     */
    public function getTipo()
    {
        return $this->tipo;
    }

    /**
     * Set testopadre
     *
     * @param string $testopadre
     * @return Llppmenutop
     */
    public function setTestopadre($testopadre)
    {
        $this->testopadre = $testopadre;

        return $this;
    }

    /**
     * Get testopadre
     *
     * @return string 
     */
    public function getTestopadre()
    {
        return $this->testopadre;
    }

    /**
     * Set ordine
     *
     * @param integer $ordine
     * @return Llppmenutop
     */
    public function setOrdine($ordine)
    {
        $this->ordine = $ordine;

        return $this;
    }

    /**
     * Get ordine
     *
     * @return integer 
     */
    public function getOrdine()
    {
        return $this->ordine;
    }

    /**
     * Set link
     *
     * @param string $link
     * @return Llppmenutop
     */
    public function setLink($link)
    {
        $this->link = $link;

        return $this;
    }

    /**
     * Get link
     *
     * @return string 
     */
    public function getLink()
    {
        return $this->link;
    }
}
