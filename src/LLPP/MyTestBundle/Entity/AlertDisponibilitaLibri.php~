<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * AlertDisponibilitaLibri
 */
class AlertDisponibilitaLibri
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $idutente;

    /**
     * @var string
     */
    private $isbn;

    /**
     * @var boolean
     */
    private $alertsent;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set idutente
     *
     * @param integer $idutente
     * @return AlertDisponibilitaLibri
     */
    public function setIdutente($idutente)
    {
        $this->idutente = $idutente;

        return $this;
    }

    /**
     * Get idutente
     *
     * @return integer 
     */
    public function getIdutente()
    {
        return $this->idutente;
    }

    /**
     * Set isbn
     *
     * @param string $isbn
     * @return AlertDisponibilitaLibri
     */
    public function setIsbn($isbn)
    {
        $this->isbn = $isbn;

        return $this;
    }

    /**
     * Get isbn
     *
     * @return string 
     */
    public function getIsbn()
    {
        return $this->isbn;
    }

    /**
     * Set alertsent
     *
     * @param boolean $alertsent
     * @return AlertDisponibilitaLibri
     */
    public function setAlertsent($alertsent)
    {
        $this->alertsent = $alertsent;

        return $this;
    }

    /**
     * Get alertsent
     *
     * @return boolean 
     */
    public function getAlertsent()
    {
        return $this->alertsent;
    }
}
