<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * LibreriaSpessoCompratiAssieme
 */
class LibreriaSpessoCompratiAssieme
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $isbn;

    /**
     * @var string
     */
    private $listaCorrelati;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set isbn
     *
     * @param string $isbn
     * @return LibreriaSpessoCompratiAssieme
     */
    public function setIsbn($isbn)
    {
        $this->isbn = $isbn;

        return $this;
    }

    /**
     * Get isbn
     *
     * @return string 
     */
    public function getIsbn()
    {
        return $this->isbn;
    }

    /**
     * Set listaCorrelati
     *
     * @param string $listaCorrelati
     * @return LibreriaSpessoCompratiAssieme
     */
    public function setListaCorrelati($listaCorrelati)
    {
        $this->listaCorrelati = $listaCorrelati;

        return $this;
    }

    /**
     * Get listaCorrelati
     *
     * @return string 
     */
    public function getListaCorrelati()
    {
        return $this->listaCorrelati;
    }
}
