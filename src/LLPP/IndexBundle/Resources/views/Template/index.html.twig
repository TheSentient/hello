<!DOCTYPE html>
<html lang="it">
  <head>
    {% block head %}
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      {% block meta_description %}
    	  <meta name="description" content="">
      {% endblock %}
      
      {% block title %}<title>Lavori Pubblici | Informazione tecnica online</title>{% endblock %}
      <link rel="shortcut icon" href="/favicon.ico">
     
{% block stili %}
    {% stylesheets
       'assets/css/font-awesome/css/font-awesome.min.css' 
       'assets/css/style.css'
       'assets/css/bootstrap/bootstrap.css'
        filter='cssrewrite'
    output='compiled/css/frontend-framework.css' 
         %}
          <link rel="stylesheet" href="{{ asset_url }}" />
   {% endstylesheets %}

  {% endblock %}
    
      {% block js_vendors %}
        <script src="{{ asset('assets/js/jquery-2.1.3.min.js') }}"></script>
        <script src="{{ asset('assets/js/bootstrap-3.3.1-dist/js/bootstrap.min.js') }}"></script>
      {% endblock js_vendors %}


      {% block js_iesupports %}
        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
          <script src="../../assets/js/html5shiv.js"></script>
          <script src="../../assets/js/respond.min.js"></script>
        <![endif]-->
      {% endblock js_iesupports %}
  }
    {% endblock head %}
  </head>
  <body>
    {% block page_body %}
      <div id="main-nav" class="container-fluid">
        {% block main_header %}
          {{ include('LLPPIndexBundle:Home:header.html.twig') }}
        {% endblock %}
      </div>
      <div id="main-content" class="container">
      {% block main_content %}
        {% block page_header %}
          {{ include('LLPPIndexBundle:Home:page-header.html.twig') }}
        {% endblock %}

        <div id="page-content">
           {% block page_content %}
           {% endblock %}
        </div>
      {% endblock main_content %}
      </div>
    {% endblock page_body %}
    <modal></modal>
    <script src="{{ asset('assets/js/unico.js') }}"></script>
  </body>
</html>