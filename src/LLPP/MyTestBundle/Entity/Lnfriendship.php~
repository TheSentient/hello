<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Lnfriendship
 */
class Lnfriendship
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $id1;

    /**
     * @var integer
     */
    private $id2;

    /**
     * @var \DateTime
     */
    private $data;

    /**
     * @var string
     */
    private $reale;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set id1
     *
     * @param integer $id1
     * @return Lnfriendship
     */
    public function setId1($id1)
    {
        $this->id1 = $id1;

        return $this;
    }

    /**
     * Get id1
     *
     * @return integer 
     */
    public function getId1()
    {
        return $this->id1;
    }

    /**
     * Set id2
     *
     * @param integer $id2
     * @return Lnfriendship
     */
    public function setId2($id2)
    {
        $this->id2 = $id2;

        return $this;
    }

    /**
     * Get id2
     *
     * @return integer 
     */
    public function getId2()
    {
        return $this->id2;
    }

    /**
     * Set data
     *
     * @param \DateTime $data
     * @return Lnfriendship
     */
    public function setData($data)
    {
        $this->data = $data;

        return $this;
    }

    /**
     * Get data
     *
     * @return \DateTime 
     */
    public function getData()
    {
        return $this->data;
    }

    /**
     * Set reale
     *
     * @param string $reale
     * @return Lnfriendship
     */
    public function setReale($reale)
    {
        $this->reale = $reale;

        return $this;
    }

    /**
     * Get reale
     *
     * @return string 
     */
    public function getReale()
    {
        return $this->reale;
    }
}
