<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ArchivioOfferte
 */
class ArchivioOfferte
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $sito;

    /**
     * @var string
     */
    private $isbn;

    /**
     * @var \DateTime
     */
    private $dataScadenza;

    /**
     * @var string
     */
    private $stoffOfferta;

    /**
     * @var string
     */
    private $stoffSpedizione;

    /**
     * @var float
     */
    private $stoffPrezzoOfferta;

    /**
     * @var string
     */
    private $stiniOfferta;

    /**
     * @var string
     */
    private $stiniSpedizione;

    /**
     * @var float
     */
    private $stiniPrezzoOfferta;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set sito
     *
     * @param string $sito
     * @return ArchivioOfferte
     */
    public function setSito($sito)
    {
        $this->sito = $sito;

        return $this;
    }

    /**
     * Get sito
     *
     * @return string 
     */
    public function getSito()
    {
        return $this->sito;
    }

    /**
     * Set isbn
     *
     * @param string $isbn
     * @return ArchivioOfferte
     */
    public function setIsbn($isbn)
    {
        $this->isbn = $isbn;

        return $this;
    }

    /**
     * Get isbn
     *
     * @return string 
     */
    public function getIsbn()
    {
        return $this->isbn;
    }

    /**
     * Set dataScadenza
     *
     * @param \DateTime $dataScadenza
     * @return ArchivioOfferte
     */
    public function setDataScadenza($dataScadenza)
    {
        $this->dataScadenza = $dataScadenza;

        return $this;
    }

    /**
     * Get dataScadenza
     *
     * @return \DateTime 
     */
    public function getDataScadenza()
    {
        return $this->dataScadenza;
    }

    /**
     * Set stoffOfferta
     *
     * @param string $stoffOfferta
     * @return ArchivioOfferte
     */
    public function setStoffOfferta($stoffOfferta)
    {
        $this->stoffOfferta = $stoffOfferta;

        return $this;
