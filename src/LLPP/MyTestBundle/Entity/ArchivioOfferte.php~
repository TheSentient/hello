        return $this->stoffPrezzoOfferta;
    }

    /**
     * Set stiniOfferta
     *
     * @param string $stiniOfferta
     * @return ArchivioOfferte
     */
    public function setStiniOfferta($stiniOfferta)
    {
        $this->stiniOfferta = $stiniOfferta;

        return $this;
    }

    /**
     * Get stiniOfferta
     *
     * @return string 
     */
    public function getStiniOfferta()
    {
        return $this->stiniOfferta;
    }

    /**
     * Set stiniSpedizione
     *
     * @param string $stiniSpedizione
     * @return ArchivioOfferte
     */
    public function setStiniSpedizione($stiniSpedizione)
