<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PhpbbSearchWordlist
 */
class PhpbbSearchWordlist
{
    /**
     * @var string
     */
    private $wordText;

    /**
     * @var integer
     */
    private $wordId;

    /**
     * @var boolean
     */
    private $wordCommon;


    /**
     * Get wordText
     *
     * @return string 
     */
    public function getWordText()
    {
        return $this->wordText;
    }

    /**
     * Set wordId
     *
     * @param integer $wordId
     * @return PhpbbSearchWordlist
     */
    public function setWordId($wordId)
    {
        $this->wordId = $wordId;

        return $this;
    }

    /**
     * Get wordId
     *
     * @return integer 
     */
    public function getWordId()
    {
        return $this->wordId;
    }

    /**
     * Set wordCommon
     *
     * @param boolean $wordCommon
     * @return PhpbbSearchWordlist
     */
    public function setWordCommon($wordCommon)
    {
        $this->wordCommon = $wordCommon;

        return $this;
    }

    /**
     * Get wordCommon
     *
     * @return boolean 
     */
    public function getWordCommon()
    {
        return $this->wordCommon;
    }
}
