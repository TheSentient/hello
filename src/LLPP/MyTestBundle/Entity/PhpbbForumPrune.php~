<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PhpbbForumPrune
 */
class PhpbbForumPrune
{
    /**
     * @var integer
     */
    private $pruneId;

    /**
     * @var integer
     */
    private $forumId;

    /**
     * @var integer
     */
    private $pruneDays;

    /**
     * @var integer
     */
    private $pruneFreq;


    /**
     * Get pruneId
     *
     * @return integer 
     */
    public function getPruneId()
    {
        return $this->pruneId;
    }

    /**
     * Set forumId
     *
     * @param integer $forumId
     * @return PhpbbForumPrune
     */
    public function setForumId($forumId)
    {
        $this->forumId = $forumId;

        return $this;
    }

    /**
     * Get forumId
     *
     * @return integer 
     */
    public function getForumId()
    {
        return $this->forumId;
    }

    /**
     * Set pruneDays
     *
     * @param integer $pruneDays
     * @return PhpbbForumPrune
     */
    public function setPruneDays($pruneDays)
    {
        $this->pruneDays = $pruneDays;

        return $this;
    }

    /**
     * Get pruneDays
     *
     * @return integer 
     */
    public function getPruneDays()
    {
        return $this->pruneDays;
    }

    /**
     * Set pruneFreq
     *
     * @param integer $pruneFreq
     * @return PhpbbForumPrune
     */
    public function setPruneFreq($pruneFreq)
    {
        $this->pruneFreq = $pruneFreq;

        return $this;
    }

    /**
     * Get pruneFreq
     *
     * @return integer 
     */
    public function getPruneFreq()
    {
        return $this->pruneFreq;
    }
}
