<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Prezzari
 */
class Prezzari
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $titolo;

    /**
     * @var string
     */
    private $link;

    /**
     * @var string
     */
    private $tipolink;

    /**
     * @var integer
     */
    private $contatore;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set titolo
     *
     * @param string $titolo
     * @return Prezzari
     */
    public function setTitolo($titolo)
    {
        $this->titolo = $titolo;

        return $this;
    }

    /**
     * Get titolo
     *
     * @return string 
     */
    public function getTitolo()
    {
        return $this->titolo;
    }

    /**
     * Set link
     *
     * @param string $link
     * @return Prezzari
     */
    public function setLink($link)
    {
        $this->link = $link;

        return $this;
    }

    /**
     * Get link
     *
     * @return string 
     */
    public function getLink()
    {
        return $this->link;
    }

    /**
     * Set tipolink
     *
     * @param string $tipolink
     * @return Prezzari
     */
    public function setTipolink($tipolink)
    {
        $this->tipolink = $tipolink;

        return $this;
    }

    /**
     * Get tipolink
     *
     * @return string 
     */
    public function getTipolink()
    {
        return $this->tipolink;
    }

    /**
     * Set contatore
     *
     * @param integer $contatore
     * @return Prezzari
     */
    public function setContatore($contatore)
    {
        $this->contatore = $contatore;

        return $this;
    }

    /**
     * Get contatore
     *
     * @return integer 
     */
    public function getContatore()
    {
        return $this->contatore;
    }
}
