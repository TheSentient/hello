<?php

namespace LLPP\MyTestBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * TipologieNewsletter
 */
class TipologieNewsletter
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $codice;

    /**
     * @var string
     */
    private $sito;

    /**
     * @var string
     */
    private $tipo;

    /**
     * @var string
     */
    private $template;

    /**
     * @var string
     */
    private $startPage;

    /**
     * @var string
     */
    private $mittente;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set codice
     *
     * @param string $codice
     * @return TipologieNewsletter
     */
    public function setCodice($codice)
    {
        $this->codice = $codice;

        return $this;
    }

    /**
     * Get codice
     *
     * @return string 
     */
    public function getCodice()
    {
        return $this->codice;
    }

    /**
     * Set sito
     *
     * @param string $sito
     * @return TipologieNewsletter
     */
    public function setSito($sito)
    {
        $this->sito = $sito;

        return $this;
    }

    /**
     * Get sito
     *
     * @return string 
     */
    public function getSito()
    {
        return $this->sito;
    }

    /**
     * Set tipo
     *
     * @param string $tipo
     * @return TipologieNewsletter
     */
    public function setTipo($tipo)
    {
        $this->tipo = $tipo;

        return $this;
    }

    /**
     * Get tipo
     *
     * @return string 
     */
    public function getTipo()
    {
        return $this->tipo;
    }

    /**
     * Set template
     *
     * @param string $template
     * @return TipologieNewsletter
     */
    public function setTemplate($template)
    {
        $this->template = $template;

        return $this;
    }

    /**
     * Get template
     *
     * @return string 
     */
    public function getTemplate()
    {
        return $this->template;
    }

    /**
     * Set startPage
     *
     * @param string $startPage
     * @return TipologieNewsletter
     */
    public function setStartPage($startPage)
    {
        $this->startPage = $startPage;

        return $this;
    }

    /**
     * Get startPage
     *
     * @return string 
     */
    public function getStartPage()
    {
        return $this->startPage;
    }

    /**
     * Set mittente
     *
     * @param string $mittente
     * @return TipologieNewsletter
     */
    public function setMittente($mittente)
    {
        $this->mittente = $mittente;

        return $this;
    }

    /**
     * Get mittente
     *
     * @return string 
     */
    public function getMittente()
    {
        return $this->mittente;
    }
}
